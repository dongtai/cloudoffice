<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="backflow" namespace="/backflow" extends="struts-default">
        <action name="state_*" method="{1}" class="backflow.StateWork">
            <result name="input" >/backflow/stateadd.jsp</result>
            <result name="selectman" >/backflow/stateselectman.jsp</result>
            <result name="search" >/backflow/statesearch.jsp</result>
            <result name="success" type="redirectAction">input</result>
        </action>
        
        <action name="type_*" method="{1}" class="backflow.TypeWork">
        	<result name="input">/backflow/typeadd.jsp</result>
        	<result name="success" type="redirectAction">input</result>
        </action>
        
		<action name="flowinfo_*" method="{1}" class="backflow.FlowInfoWork">
            <result name="input" >/backflow/flowinfoadd.jsp</result>
            <result name="success" type="redirectAction">input</result>
        </action>
        <action name="flownode_*" method="{1}" class="backflow.FlowNodeWork">
            <result name="input" >/backflow/flownode.jsp</result>
            <result name="selectman" >/backflow/flowselectman.jsp</result>
            <result name="viewpicture" >/backflow/viewpicture.jsp</result>
            <result name="subnode" >/backflow/flowsubnode.jsp</result>
            <result name="selectcond" >/backflow/selectcond.jsp</result>
            <result name="success" type="redirectAction">input</result>
        </action>
        <action name="flowsubnode_*" method="{1}" class="backflow.FlowSubNodeWork">
            <result name="input" >/backflow/flowsubnode.jsp</result>
            <result name="selectman" >/backflow/flowsubman.jsp</result>
            <result name="success" type="redirectAction">input</result>
        </action>
        
        <action name="*" class="backflow.AllSupport">
            <result>/backflow/{1}.jsp</result>
        </action>
    </package>
</struts>
