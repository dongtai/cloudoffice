<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yozo.report.commons.domain.databaseObject">
	<class name="DataType" table="rpt_sys_dataType">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="name">
			<column name="name" length="64" not-null="true">
				<comment>数据类型名称</comment>
			</column>
		</property>
		<property name="basicType">
			<column name="basicType" not-null="true">
				<comment>基本类型</comment>
			</column>
		</property>
		<property name="attrFlag">
			<column name="attrFlag">
				<comment>基本属性集</comment>
			</column>
		</property>
		<property name="limitLength">
			<column name="limitLength">
				<comment>限定长度</comment>
			</column>
		</property>
		<property name="decimalDigit">
			<column name="decimalDigit">
				<comment>小数位数</comment>
			</column>
		</property>
		<property name="matchPattern">
			<column name="matchPattern">
				<comment>匹配值</comment>
			</column>
		</property>
		<property name="description">
			<column name="description">
				<comment>说明</comment>
			</column>
		</property>
		<property name="creatorId">
			<column name="creatorId" not-null="true">
				<comment>创建者ID</comment>
			</column>
		</property>
		<property name="createDate" not-null="true">
			<column name="createDate">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="modifierId">
			<column name="modifierId">
				<comment>修改者ID</comment>
			</column>
		</property>
		<property name="modifyDate">
			<column name="modifyDate">
				<comment>修改时间</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
