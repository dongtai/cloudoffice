<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.yozo.report.commons.domain.databaseObject.ListSelectItem" table="rpt_sys_listSelectItem">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="attrFlag">
			<column name="attrFlag">
				<comment>基本属性集</comment>
			</column>
		</property>
		<property name="position">
			<column name="position">
				<comment>位置</comment>
			</column>
		</property>
		<many-to-one name="dfield">
			<column name="dfield_id">
				<comment>对应的字段id</comment>
			</column>
		</many-to-one>
		<property name="sortType">
			<column name="sortType">
				<comment>排序类型</comment>
			</column>
		</property>
		<property name="showName">
			<column name="showName">
				<comment>显示名称</comment>
			</column>
		</property>
		<many-to-one name="treeSelectRule" cascade="all">
			<column name="treeSelectRule_id">
				<comment>树型选择规范id</comment>
			</column>
		</many-to-one>
		<property name="associationType">
			<column name="associationType">
				<comment>关联类型</comment>
			</column>
		</property>
		<many-to-one name="listSelectRule">
			<column name="listSelectRule_id">
				<comment>列表选择规范id</comment>
			</column>
		</many-to-one>
	</class>
</hibernate-mapping>
