<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yozo.report.commons.domain.databaseObject">
	<class name="FillMode" table="rpt_sys_fillmode">
		<id name="id">
			<generator class="native" />
		</id>
		<many-to-one name="rtable">
			<column name="rtable_id">
				<comment>填充到的报表</comment>
			</column>
		</many-to-one>
		<set name="fillModeItems" cascade="all" lazy="false">
			<key>
				<column name="fillMode_id">
					<comment>填充方式项</comment>
				</column>
			</key>
			<one-to-many class="FillModeItem" />
		</set>
		<property name="attrFlag">
			<column name="attrFlag">
				<comment>基本属性集</comment>
			</column>
		</property>
		<many-to-one name="readRule">
			<column name="readRule_id">
				<comment>提数规则id</comment>
			</column>
		</many-to-one>
	</class>
</hibernate-mapping>
