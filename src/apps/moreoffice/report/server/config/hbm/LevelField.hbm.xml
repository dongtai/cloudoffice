<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.yozo.report.commons.domain.databaseObject.LevelField" table="rpt_sys_levelfield">
		<id name="id">
			<generator class="native" />
		</id>
		<many-to-one name="dfield">
			<column name="dfield_id">
				<comment>分级字段id</comment>
			</column>
		</many-to-one>
		<property name="sortType">
			<column name="sortType">
				<comment>排序方式</comment>
			</column>
		</property>
		<property name="position">
			<column name="position">
				<comment>位置</comment>
			</column>
		</property>
		<many-to-one name="treeSelectRule">
			<column name="treeSelectRule_id">
				<comment>树型选择规范id</comment>
			</column>
		</many-to-one>
	</class>
</hibernate-mapping>
