<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yozo.report.commons.domain.databaseObject">
	<class name="WriteMode" table="rpt_sys_writemode">
		<id name="id">
			<generator class="native" />
		</id>
		<many-to-one name="rtable">
			<column name="rtable_id">
				<comment>回写到的报表</comment>
			</column>
		</many-to-one>
		<set name="writeModeItems" cascade="all" lazy="false">
			<key>
				<column name="writeMode_id">
					<comment>回写</comment>
				</column>
			</key>
			<one-to-many class="WriteModeItem" />
		</set>
		<many-to-one name="newFormRule">
			<column name="newFormRule_id">
				<comment>新建表单规则id</comment>
			</column>
		</many-to-one>
	</class>
</hibernate-mapping>
