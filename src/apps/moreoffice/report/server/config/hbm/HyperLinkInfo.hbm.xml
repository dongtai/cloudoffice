<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.yozo.report.commons.domain.databaseObject.HyperLinkInfo" table="rpt_sys_hyperlink_info">
		<id name="id">
			<generator class="native" />
		</id>
		<many-to-one name="currentField">
			<column name="currentField_id">
				<comment>本报表字段id</comment>
			</column>
		</many-to-one>
		<many-to-one name="targetField">
			<column name="targetField_id">
				<comment>目标模板上的字段id</comment>
			</column>
		</many-to-one>
		<property name="urlParam">
			<column name="urlParam">
				<comment>URL参数</comment>
			</column>
		</property>
		<property name="attrFlag">
			<column name="attrFlag">
				<comment>基本属性集</comment>
			</column>
		</property>
		<many-to-one name="hyperLink">
			<column name="hyperLink_id">
				<comment>超链接id</comment>
			</column>
		</many-to-one>
	</class>
</hibernate-mapping>
