<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yozo.report.commons.domain.databaseObject">
	<class name="JoinCond" table="rpt_sys_joinCond">
		<id name="id">
			<generator class="native" />
		</id>
		<many-to-one name="leftTable">
			<column name="leftTable_id">
				<comment>左表id</comment>
			</column>
		</many-to-one>
		<many-to-one name="leftField">
			<column name="leftField_id">
				<comment>左字段id</comment>
			</column>
		</many-to-one>
		<property name="relation">
			<column name="relation">
				<comment>关系</comment>
			</column>
		</property>
		<many-to-one name="rightTable">
			<column name="rightTable_id">
				<comment>右表id</comment>
			</column>
		</many-to-one>
		<many-to-one name="rightField">
			<column name="rightField_id">
				<comment>右字段id</comment>
			</column>
		</many-to-one>
		<many-to-one name="readRule">
			<column name="readRule_id">
				<comment>提数规则id</comment>
			</column>
		</many-to-one>
	</class>
</hibernate-mapping>
