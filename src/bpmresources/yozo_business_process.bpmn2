    </endEvent>
    <exclusiveGateway id="_8" name="Gateway" gatewayDirection="Converging" />
    <inclusiveGateway id="_9" name="Gateway" gatewayDirection="Diverging" />

    <!-- connections -->
    <sequenceFlow id="_1-_2" sourceRef="_1" targetRef="_2" />
    <sequenceFlow id="_8-_3" sourceRef="_8" targetRef="_3" />
    <sequenceFlow id="_3-_4" sourceRef="_3" targetRef="_4" />
    <sequenceFlow id="_4-_5" sourceRef="_4" targetRef="_5" />
    <sequenceFlow id="_9-_6" sourceRef="_9" targetRef="_6" name="pass" >
      <conditionExpression xsi:type="tFormalExpression" >return auditStatus;</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_6-_7" sourceRef="_6" targetRef="_7" />
    <sequenceFlow id="_9-_8" sourceRef="_9" targetRef="_8" name="nopass" >
      <conditionExpression xsi:type="tFormalExpression" >return !auditStatus;</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_2-_8" sourceRef="_2" targetRef="_8" />
    <sequenceFlow id="_5-_9" sourceRef="_5" targetRef="_9" />

