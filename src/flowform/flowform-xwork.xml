<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="flowform" namespace="/flowform" extends="struts-default">
        <action name="flowform_*" method="{1}" class="flowform.FlowFormWork">
        	<result name="addflowform" >/flowform/flowform.jsp</result>
            <result name="input" >/flowform/flowlist.jsp</result>
            <result name="uploadfile" >/flowform/uploadfile.jsp</result>
            <result name="uploadsuccess" >/flowform/uploadsuccess.jsp</result>
            <result name="formview" >/flowform/flowformview.jsp</result>
            <result name="formlist" >/flowform/flowformlist.jsp</result>
            <result name="modifyflowform" >/flowform/modifyflowform.jsp</result>
            <result name="searchflow" >/flowform/searchflow.jsp</result>
            <result name="selectman" >/flowform/flowformselectman.jsp</result>
            <result name="successclose">/flowform/close.jsp</result>
            <result name="success" type="redirectAction">input</result>
        </action>
        
        
        <action name="*" class="flowform.AllSupport">
            <result>/flowform/{1}.jsp</result>
        </action>
    </package>
</struts>
